{
  "name": "Iorad Confluence Cloud Plugin",
  "description": "Iorad Atlassian Connect add-on",
  "key": "com.iorad.conf-cloud-plugin",
  "baseUrl": "https://www.iorad.com/assets/confluence/",
  "version": "2.0",
  "vendor": {
    "name": "iorad inc.",
    "url": "https://www.iorad.com"
  },
  "authentication": {
    "type": "none"
  },
  "apiVersion": 1,

  "apiMigrations": {
    "gdpr": true
  },

  "modules": {
    "dynamicContentMacros": [{
      "width": "100%",
      "height": "700px",

      "url": "/iorad.html?pageTitle={page.title}&iframeUrl={iframeUrl}&embedCode={embedCode}&width={width}&height={height}&macroId={macro.id}",
      "description": {
        "value": "Capture Solution for your articles."
      },
      "icon": {
        "width": 64,
        "height": 64,
        "url": "https://www.iorad.com/assets/confluence/icon.png"
      },
      "imagePlaceholder": {
        "width": 392,
        "height": 110,
        "url": "https://www.iorad.com/assets/confluence/tutorial_embedded.png",
        "applyChrome": true
      },
      "documentation": {
        "url": "https://www.iorad.com"
      },
      "categories": [
        "visuals"
      ],
      "outputType": "block",
      "bodyType": "none",
      "aliases": [
        "map"
      ],
      "featured": true,

      "parameters": [{
        "identifier": "iframeUrl",
        "name": {
          "value": "Embed Url"
        },
        "description": {
          "value": "This is embed url of the solution."
        },
        "type": "string",
        "required": false,
        "multiple": false,
        "defaultValue": ""
      }, {
        "identifier": "width",
        "name": {
          "value": "Embed frame width"
        },
        "description": {
          "value": "This is embed width of the frame in the article"
        },
        "type": "string",
        "required": false,
        "multiple": false,
        "defaultValue": ""
      }, {
        "identifier": "height",
        "name": {
          "value": "Embed frame height Url"
        },
        "description": {
          "value": "This is embed height of the frame in the article"
        },
        "type": "string",
        "required": false,
        "multiple": false,
        "defaultValue": ""
      }],

      "key": "iorad-conf-plugin",
      "name": {
        "value": "Embed iorad"
      },

      "editor": {
        "url": "/iorad_start.html",
        "editTitle": {
          "value": "Edit Solution",
          "i18n": "macro.md.edit"
        },
        "insertTitle": {
          "value": "Capture Solution",
          "i18n": "macro.md.insert"
        },
        "width": "100%",
        "height": "100%"
      }

    }]

  }

}
